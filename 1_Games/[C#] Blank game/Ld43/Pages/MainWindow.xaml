<Window	x:Class="Ld43.Pages.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:Ld43.Pages"
		mc:Ignorable="d"

		Icon="..\Resources\Textures\icon.png"
		Title="{DynamicResource gameTitle}"
		Height="{Binding Source={StaticResource Settings}, Path=WindowHeight}"
        Width="{Binding Source={StaticResource Settings}, Path=WindowWidth}"
        
        WindowStartupLocation="CenterScreen"
		Closed="Window_Closed"
		>
	<local:MainMenuPage/>
</Window>
